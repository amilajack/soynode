{
  "ignore": ["node_modules"],
  "presets": [
    ["@babel/preset-env", { "targets": { "node": 8 }, "useBuiltIns": "usage" }],
    "@babel/preset-flow"
  ],
  "plugins": [
    "add-module-exports",
    "dynamic-import-node",
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-proposal-class-properties"
  ]
}
